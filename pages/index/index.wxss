/* diy组件 */
@import "../../components/diy/index.wxss";

.placeholder {
  width: 100%;
  font-size: 14px;
  color: #999;
}

.container {
  background-color: #F7F8F9;
  overflow: hidden;
  padding-bottom: 180rpx !important;
}

.weui-flex {
  margin-bottom: 20rpx;
  background-color: white;
}


.placeholder image {
  width: 80px;
  height: 80px;
  border-radius: 8px;
}

.store__info {
  padding-left: 5px;
}

.store__info .placeholder {
  height: 20px;
  font-size: 14px;
}

.store__info {
  overflow: hidden;
  white-space: normal;
}

.store__info .store__name {
  font-size: 28rpx;
  font-weight: bold;
  line-height: 44rpx;
  color: #333;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  width: 100%;
}

.store__list {
  margin-top: 20rpx;
}

.navigator {
  height: 80px;
  padding: 0 20px;
  text-align: center;
  border-left: 1px solid #eee;
}

.navigator .icon-daohang {
  font-size: 60rpx;
  line-height: 50px;
  color: rgb(77, 169, 255);
}

.navigator .placeholder {
  font-size: 24rpx;
  color: rgb(77, 169, 255);
}

.icon-daohang {
  font-size: 44rpx;
}

.weui-flex {
  padding: 0 10px;
}

.index__nail-map {
  background-color: white;
  display: inline-block;
  width: 100rpx;
  height: 100rpx;
  position: fixed;
  right: 20rpx;
  bottom: 120rpx;
  line-height: 120rpx;
  text-align: center;
  font-size: 24rpx;
  border-radius: 4px;
  z-index: 999;
}

.index__nail-map image {
  height: 100rpx;
  width: 100rpx;
}

.fake-input {
  height: 36px;
  width: 90%;
  border: 1px solid #eee;
  border-radius: 8rpx;
  background-color: #eee;
  margin: 10px auto;
  color: #999;
  line-height: 36px;

}

.fake-input navigator {
  padding: 0 10px;
  font-size: 28rpx;
}

.page_title {
  width: 100%;
  height: 88rpx;
  background: #3786FF;
  border-radius: 0rpx 0rpx 0rpx 0rpx;
  opacity: 1;
  font-size: 40rpx;
  font-family: OPPOSans-Heavy, OPPOSans;
  font-weight: 900;
  color: #FFFFFF;
  line-height: 88rpx;
  text-indent: 38rpx;
}

.head_title {
  width: 100%;
  height: 400rpx;
  border-radius: 0rpx 0rpx 0rpx 0rpx;
  opacity: 1;
  font-size: 40rpx;
  font-family: OPPOSans-Heavy, OPPOSans;
  font-weight: 900;
  color: #FFFFFF;
  line-height: 88rpx;
  text-indent: 38rpx;
  background: linear-gradient(180deg, #3786FF 30%, rgba(0, 101, 255, 0) 70%);
  /* position: absolute; */
}

.custom_nav {
  background: #3786FF;
}

.banner {
  width: 100%;
  /* padding-top: 46rpx; */
  border-radius: 0rpx 0rpx 0rpx 0rpx;
  opacity: 1;
  margin-top: -400rpx;
  margin-bottom: 30rpx;
}

.banner swiper {
  height: 327rpx;
}

.slide-image {
  width: 100%;
  height: 327rpx;
  /* border-radius: 16rpx 16rpx 16rpx 16rpx; */
}

.appointment_time {
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
  margin-bottom: 32rpx;
}

.business_hours {
  width: 344rpx;
  height: 144rpx;
  background: #FDF0E3;
  border-radius: 12rpx 12rpx 12rpx 12rpx;
  opacity: 1;
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.business_hours:nth-last-of-type(1) {
  background: #EBF1FD;
}

.hours {
  font-size: 40rpx;
  font-family: OPPOSans-Bold, OPPOSans;
  font-weight: 700;
  /* color: #E55923; */
}

.week {
  font-size: 26rpx;
  font-family: OPPOSans-Medium, OPPOSans;
  font-weight: 500;
  /* color: #C87859; */
  line-height: 16px;
}

.crisscross {
  width: 100rpx;
  height: 100rpx;
}

.booking_register {
  width: 344rpx;
  height: 144rpx;
  background: #EBF1FD;
  border-radius: 12rpx 12rpx 12rpx 12rpx;
  opacity: 1;
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.booking {
  font-size: 40rpx;
  font-family: OPPOSans-Bold, OPPOSans;
  font-weight: 700;
  color: #2868FF;
}

.line {
  font-size: 32rpx;
  font-family: OPPOSans-Medium, OPPOSans;
  font-weight: 500;
  color: #6390E3;
}

.doctor {
  width: 100rpx;
  height: 100rpx;
}

.doctor_project {
  width: 144rpx;
  height: 144rpx;
}

.doctor_referral {
  font-size: 32rpx;
  font-family: OPPOSans-Medium, OPPOSans;
  font-weight: 500;
  color: #676767;
}

.announcement {
  width: 708rpx;
  height: 122rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 8rpx 20rpx 0rpx rgba(214, 220, 243, 0.4);
  border-radius: 12rpx 12rpx 12rpx 12rpx;
  opacity: 1;
  margin: 0 auto 32rpx;
  display: flex;
  /* justify-content: space-around; */
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.announcement_png {
  width: 94rpx;
  height: 94rpx;
  flex-shrink: 0;
  margin-right: 20rpx;
}

.announcement_title {
  font-size: 30rpx;
  font-family: OPPOSans-Medium, OPPOSans;
  font-weight: 500;
  color: #707481;
  width: 500rpx;
}

.project {
  display: flex;
  justify-content: space-around;
  width: 708rpx;
  height: 200rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 8rpx 20rpx 0rpx rgba(214, 220, 243, 0.4);
  border-radius: 12rpx 12rpx 12rpx 12rpx;
  opacity: 1;
  margin: 0 auto 32rpx;
}

.project_content {
  display: flex;
  flex-direction: column;
  align-items: center;
  line-height: 20rpx;
}

.goods_list {
  width: 708rpx;
  margin: 0 auto 32rpx;
}

.goods_interface {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 36rpx;
  background: #fff;
  border-radius: 12rpx;
}

.goods_png {
  width: 708rpx;
  height: 708rpx;
  border-radius: 12rpx 12rpx 0rpx 0rpx;
}

.goods_introduce {
  width: 708rpx;
  height: 210rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 8rpx 20rpx 0rpx rgba(214, 220, 243, 0.4);
  border-radius: 0rpx 0rpx 12rpx 12rpx;
  opacity: 1;
}

.goods_price {
  display: flex;
  align-items: center;
  margin: 26rpx 0 0 28rpx;
}

.goods_price span:nth-child(1) {
  font-size: 24rpx;
  font-family: OPPOSans-Bold, OPPOSans;
  font-weight: 700;
  color: #FF0000;
}

.goods_price span:nth-child(2) {
  font-size: 44rpx;
  font-family: OPPOSans-Heavy, OPPOSans;
  font-weight: 900;
  color: #FF0000;
}

.goods_price span:nth-child(3) {
  width: 122rpx;
  height: 34rpx;
  display: inline-block;
  text-align: center;
  background: #FFE8E9;
  border-radius: 4rpx 4rpx 4rpx 4rpx;
  opacity: 1;
  font-size: 20rpx;
  font-family: OPPOSans-Regular, OPPOSans;
  font-weight: 400;
  color: #FF0000;
  margin-left: 15rpx;
}

.goods_title {
  width: 640rpx;
  height: 96rpx;
  font-size: 40rpx;
  font-family: OPPOSans-Bold, OPPOSans;
  font-weight: 700;
  color: #333333;
  line-height: 48rpx;
  margin: 0 0 26rpx 28rpx;
}

.fixed {
  position: fixed;
  width: 100%;
  z-index: 10;
}

.background_gradient {
  width: 100%;
  height: 88rpx;
  /* background: linear-gradient(180deg, #3786FF 30%, rgba(0,101,255,0) 100%); */
}

.fix-contact {
  position: fixed;
  bottom: 50rpx;
  right: 50rpx;
  background: transparent;
  width: 100rpx;
  height: 100rpx;
  /* background: #eee; */
  border-radius: 50%;
  padding: 0;
}

.fix-contact .fix_button {
  width: 100rpx;
  height: 100rpx;
}

.doctor_list {
  width: 708rpx;
  background: #FFFFFF;
  border-radius: 12rpx 12rpx 12rpx 12rpx;
  margin: 0 auto 32rpx;
  padding: 0 32rpx;
  box-sizing: border-box;
}

.doctor_list_title {
  padding: 20rpx;
  font-size: 44rpx;
  font-weight: 700;
  color: #3D3D3D;
  /* border-bottom: 1rpx solid #E9E9E9; */
}

.doctor_list_item {
  display: flex;
  padding: 16rpx 0;
  border-top: 1rpx solid #E9E9E9;
  align-items: center;
}

.doctor_list_item .avatar {
  width: 136rpx;
  height: 136rpx;
  flex-shrink: 0;
  margin-right: 20rpx;
}

.doctor_list_item_sub_title {
  font-size: 40rpx;
  color: #353535;
  font-weight: 700;
}

.doctor_list_item_title {
  font-size: 28rpx;
  color: #444;
}

.doctor_list_item_info {
  font-size: 28rpx;
  font-weight: 400;
  color: #8B8B8B;
}

.doctor_list_item_btn {
  flex-shrink: 0;
}

.doctor_list_item_btn .van-button {
  border-radius: 12rpx !important;
  margin-left: 10rpx;
}

.van-dialog__message {
  text-align: left !important;
}

.dialog_wrap {
  max-height: 800rpx;
  overflow: auto;
}

.dialog_img {
  width: 100%;
}